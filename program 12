#include <iostream> 
using namespace std; 
   class Student { 
private: 
    string name; 
    int age; 
public: 
 Student()  
{ 
 name = "swathi"; 
 age = 19; 
  } 
  Student(string n) { 
name = n; 
age = 10; 
} 
  Student(string n, int a) { 
  name = n; 
  age = a; 
  } 
   void display() { 
        cout << "Name: " << name << ", Age: " << age << endl; 
    } 
}; 
int main() { 
    Student s1;                
    Student s2("jenny");       
    Student s3("jhon", 20);   
       s1.display(); 
    s2.display(); 
s3.display(); 
return 0; 
}
