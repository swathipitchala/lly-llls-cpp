#include <iostream>
using namespace std;
class Student {
private:
    string name;
    int age;
public:
    void setDetails(string n, int a) {
        name = n;
        age = a;
    }
    void display() {
        cout << "Name: " << name << ", Age: " << age << endl;
    }
};
int main() {
    Student* ptr = new Student();
    ptr->setDetails("swathi", 19);
    ptr->display();
    delete ptr;
    return 0;
}
