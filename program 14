#include <iostream>
using namespace std;
class Number {
    int value;
public:
    Number(int v = 0) : value(v) {}
    // Unary operator overloading (negation)
    Number operator-() const {
        return Number(-value);
    }
    // Binary operator overloading (addition)
    Number operator+(const Number& other) const {
        return Number(value + other.value);
    }
    void display() const {
        cout << "Value: " << value << endl;
    }
};
int main() {
    Number n1(10), n2(20);
    Number n3 = -n1;      // Unary operator
    Number n4 = n1 + n2;  // Binary operator
    cout << "After unary minus: ";
    n3.display();
    cout << "After addition: ";
    n4.display();
return 0;
}



